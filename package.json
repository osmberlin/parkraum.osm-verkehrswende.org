{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run fetchRegions && astro dev",
    "start": "npm run dev",
    "build": "npm run fetchRegions && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "fetchRegions": "vite-node ./data/fetchRegions.ts && prettier ./data/regions.json --write",
    "type-check": "astro check && tsc --skipLibCheck",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|astro|mdx)\" --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.12.1",
    "@astrojs/alpinejs": "^0.2.1",
    "@astrojs/mdx": "^0.19.1",
    "@astrojs/react": "^2.1.3",
    "@astrojs/rss": "^2.4.1",
    "@astrojs/sitemap": "^1.3.1",
    "@astrojs/tailwind": "^3.1.2",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.29.5",
    "@turf/turf": "^6.5.0",
    "@types/alpinejs": "^3.7.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "alpinejs": "^3.12.1",
    "astro": "^2.4.5",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-toc": "^8.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-node": "^0.31.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.1",
    "prettier-plugin-tailwindcss": "^0.2.8"
  }
